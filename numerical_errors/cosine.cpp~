double cosine(double x){
  double a_n=1; //a_0=1, el valor inicial de la sucesi√≥n
  double sum=0;
for (int i = 0; i < N; ++i)
  {
    sum += a_n;
    a_n=-x*x*a_n/(2*i+2)/(2*i+1);
  }
 return sum;
}
